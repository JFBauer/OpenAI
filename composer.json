{
    "name": "jfbauer/openai",
    "description": "This package gives you access to all the API necessary functionalities from OpenAI.",
    "type": "library",
    "authors": [
        {
            "name": "Jasper Bauer",
            "email": "jasper@jasperbauer.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^10.10",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "JFBauer\\OpenAI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JFBauer\\OpenAI\\ServiceProvider"
            ],
            "aliases": {
                "OpenAI": "ChatClient"
            }
        }
    }
}
